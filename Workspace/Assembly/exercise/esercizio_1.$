_EXIT = 1
_PRINT = 127

.SECT.TEXT
start:	MOV CX, end-vec
	MOV SI, CX
	SHR CX,1
	SUB SI,2
	MOV BX,vec
	MOV AX,0
	
1:	MOV AX, (BX)(SI) 	
	SUB SI,2
	PUSH AX
	PUSH frmt
	PUSH _PRINT
	SYS
	MOV SP,BP
	LOOP 1b
	
	PUSH frmt2
	PUSH _PRINT
	SYS
	MOV SP,BP
	
	PUSH 0
	PUSH _EXIT
	SYS

.SECT.DATA
vec: .WORD 3,4,5,6,7
end: .SPACE 1
frmt: .ASCII "%d \0"
frmt2: .ASCII "\n\0" 

.SECT.BSS
